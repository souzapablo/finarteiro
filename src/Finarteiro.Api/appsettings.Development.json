{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsfot": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://runtrackr.seq:5341" }
      }
    ],
    "Enrich": [ "FromLogConte", "WithMachineName", "WithThreadIn" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=finarteiro.database;Port=5432;Database=finarteiro-dev;User Id=postgres;Password=password@123"
  }
}
